{
  "name": "evm-express",
  "version": "0.0.2",
  "license": "MIT",
  "main": "./index.js",
  "module": "./index.esm.js",
  "source": "./index.ts",
  "types": "./types/index.d.ts",
  "description": "Express handler for EVM compatible node requests. Supports ipc, http, and websocket. Subscriptions are not supported.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KodexData/evm-express.git"
  },
  "keywords": [
    "kodexdata.systems",
    "evm",
    "ethereum",
    "express",
    "proxy",
    "handler"
  ],
  "author": "WizardsOrb <wizardsorb@kodexdata.systems>",
  "bugs": {
    "url": "https://github.com/KodexData/evm-express/issues"
  },
  "homepage": "https://github.com/KodexData/evm-express#readme",
  "scripts": {
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha --exit --timeout 10000 -r ts-node/register 'tests/**/*.ts'",
    "prettier-format": "prettier 'source/**/*.ts' --write",
    "build": "npm run clean && rollup -c",
    "clean": "rimraf types *.js *.d.ts",
    "lint": "eslint ./src --ext .ts"
  },
  "peerDependencies": {
    "express": "^4.18.1",
    "big-json": "^3.1.0",
    "ws": "^8.8.1"
  },
  "dependencies": {
    "big-json": "^3.1.0",
    "express": "^4.18.1",
    "ws": "^8.8.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.3.3",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.1.1",
    "@types/rimraf": "^3.0.2",
    "@types/rollup-plugin-peer-deps-external": "^2.2.1",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "chai": "^4.3.6",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "ganache": "^7.4.1",
    "mocha": "^10.0.0",
    "mocha-logger": "^1.0.8",
    "pg": "^8.7.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.78.1",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "endOfLine": "lf",
    "htmlWhitespaceSensitivity": "css",
    "insertPragma": false,
    "jsxBracketSameLine": false,
    "jsxSingleQuote": true,
    "printWidth": 110,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false,
    "vueIndentScriptAndStyle": false,
    "parser": "typescript"
  }
}
